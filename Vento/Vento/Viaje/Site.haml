		%meta{:charset=>"iso-8859-1"}/

		<%: Scripts.Render("~/bundles/modernizr") %>




	%body
		%form{:runat=>"server"}
			%asp:ScriptManager{:runat => "server"}
				%Scripts
					%asp:ScriptReference{:Name =>"MsAjaxBundle"}
					%asp:ScriptReference{:Name =>"jquery"}
					%asp:ScriptReference{:Name =>"jquery.ui.combined"}
					%asp:ScriptReference{:Name =>"WebForms.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/WebForms.js"}
					%asp:ScriptReference{:Name =>"WebUIValidation.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/WebUIValidation.js"}
					%asp:ScriptReference{:Name =>"MenuStandards.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/MenuStandards.js"}
					%asp:ScriptReference{:Name =>"GridView.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/GridView.js"}
					%asp:ScriptReference{:Name =>"DetailsView.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/DetailsView.js"}

					%asp:ScriptReference{:Name =>"WebParts.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/WebParts.js"}
					%asp:ScriptReference{:Name =>"Focus.js", :Assembly=>"System.Web", :Path=>"~/Scripts/WebForms/Focus.js"}
					%asp:ScriptReference{:Name =>"WebFormsBundle"}

		#leftMenuCont
			%a.link.video.active{:href=>"#intro"}
				%img{:src => "img/menu_video_icon.png", :alt => ""}/
				%p
					Video
			%a.link.mapa{:href=>"#map"}
				%img{:src => "img/menu_ruta_icon.png", :alt => ""}/
				%p
					Mapa
			%a.link.Bitacora{:href=>"#recap"}
				%img{:src => "img/menu_bitacora_icon.png", :alt => ""}/
				%p
					Bitácora
			%a.link.pruebaManejo{:href=>"#recap"}
				%img{:src => "img/menu_prueba_de_manejo.png", :alt => ""}/
				%p
					Prueba de Manejo

		#rightMenuCont
		#leftTwitter


		#preloader
			#preloader-content.preloading
				.ui-spinner
					.wheel
					.preloader-text
						Cargando...

		%header

			#logoHeader.drag
			%a#menuLeft{:href=>"#"}
				.menuExtend
		#mainCont



			/  INTRO
			.container#intro

				.logo
					.holder
						.title1
						.title2
						.title3
						.title4
						.title5
						.car
							%a.click_none.fancybox.fancybox.iframe{:href=>"http://www.youtube.com/embed/vYBUktTnRaM?autoplay=1&vq=hd1080&controls=0&rel=0&showinfo=0", :data=>{:fancybox_type=>"iframe"}}
								.state1
								.state2
								.state3
						.title6
				.share_txt
					Comparte
				.social-share

				%a.downNav.updownnav{:href=>"#map", :current=>"#intro"}

			/ MAP
			.container#map


				#zoom
					.in.jvectormap-zoomin
						+
					.out.jvectormap-zoomout
						-

				#menuRight

					%a.close{:href=>"#"}
					%a.item.itemMenu.reto{:href=>"#", :request=>"Mecanica.aspx", :currentid=>""}
					%a.item.itemMenu.ventoico#vento1{:href=>"#", :request=>"Auto.aspx", :currentid=>"1"}
						.rollOver
							%p.titlesection
								<span>#</span>
								VentoUno
							.arrow
					%a.item.itemMenu.ventoico#vento2{:href=>"#", :request=>"Auto.aspx", :currentid=>"2"}
						.rollOver
							%p.titlesection
								<span>#</span>
								VentoDos
							.arrow
					%a.item.itemMenu.ventoico#vento3{:href=>"#", :request=>"Auto.aspx", :currentid=>"3"}
						.rollOver
							%p.titlesection
								<span>#</span>
								VentoTres
							.arrow
					%a.item.grid{:href=>"#", :request=>"Muro.aspx", :currentid=>""}
				%a.upNav.updownnav{:href=>"#intro", :current=>"#map"}
				%a.downNav.updownnav{:href=>"#recap", :current=>"#map"}

				#menuHash
					%a.link{:href => "#"}
					%a.close{:href => "#"}
				#mapcontainer

			/ RECAP
			.container#recap
				%a.upNav.updownnav{:href=>"#map", :current=>"#recap"}
